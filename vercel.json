{
  "version":2,
    "builds": [
      {
        "src": "bitpoll/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config":{
          "distDir":"static"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "bitpoll/wsgi.py"
      }
    ]
  }